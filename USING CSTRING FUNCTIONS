#include <iostream>
#include <vector>
#include <cstring>

using namespace std;

int main()
{
string s1{"amonjot "};
string s2{"lives in etobicoke"};
string s3{s1+s2};
string s4{s3,0,13};
cout<<s4<<endl;
string s5{s3,14,26};
cout<<s5<<endl;
for(size_t i {0};i<s3.length();++i)
cout<<s3[i];
cout<<" "<<endl;
for(char c:s3)
	cout<<c;
return 0;
}
