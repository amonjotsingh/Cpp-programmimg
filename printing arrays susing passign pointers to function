#include<iostream>
using namespace std;
void print(int const *array,size_t size)
{
	int signed i;
	cout<<"[";
	for(i=0;i<size;i++)
	{
		cout<<*(array+i)<<" ";
	}
	cout<<"]"<<endl;
}

int main()
{
	int arr[]={1,2,3};
	int size1=3;
	print(arr,size1);

	int ar[]={10,20,30};
	int size2=3;
	print(ar,size2);

	
return 0;
}
