#include <iostream>
#include <cmath>
using namespace std;

int main()
{
double total_bill,individual_bill;
int no;
cout<<"enter total bill"<<endl;
cin>>total_bill;
cout<<"enter no of individual"<<endl;
cin>>no;
individual_bill=total_bill/no;
int indb1=ceil(individual_bill);
int indb2=round(individual_bill);
int indb3=floor(individual_bill);
cout<<"individual no ceil:"<<indb1<<" individual no round:"<<indb2<<" individual no floor:"<<indb3;
return 0;
}
